{
  "name": "remotion-audio-visualizer",
  "version": "0.1.0",
  "scripts": {
    "dev": "remotion preview",
    "render": "remotion render Main out/video.mp4",
    "test:lint": "eslint src --ext=.ts,.tsx --max-warnings=0 --format=compact",
    "test:types": "tsc",
    "upgrade": "npm i $(npm outdated | cut -d' ' -f 1 | sed '1d' | xargs -I '$' echo '$@latest' | xargs echo) --save-exact"
  },
  "dependencies": {
    "@remotion/cli": "4.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remotion": "4.0.4"
  },
  "devDependencies": {
    "@remotion/eslint-config": "4.0.4",
    "@remotion/eslint-plugin": "4.0.4",
    "@remotion/tailwind": "4.0.4",
    "@types/react": "18.2.14",
    "@types/web": "0.0.103",
    "eslint": "8.44.0",
    "eslint-config-xo": "0.43.1",
    "eslint-config-xo-react": "0.27.0",
    "eslint-config-xo-typescript": "0.57.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  }
}
